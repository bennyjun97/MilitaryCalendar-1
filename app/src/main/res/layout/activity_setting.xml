<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:theme="@style/JalnanTextViewStyle"
                                                   android:id="@+id/constraintLayout"
                                                   android:background="@drawable/cool_brown_gradient">
    <TextView
            android:text="입대일:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintTop_toTopOf="@+id/setFinalEnlist"
            app:layout_constraintStart_toStartOf="@+id/guideline14"
            android:layout_marginStart="8dp"
            android:textSize="14sp" android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/setFinalEnlist"/>
    <TextView
            android:text="이름:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="@+id/guideline14"
            android:layout_marginStart="8dp"
            android:textSize="14sp" android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/setFinalName"
            app:layout_constraintBottom_toBottomOf="@+id/setFinalName"/>
    <TextView
            android:text="소속:"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/setFinalAff"
            app:layout_constraintStart_toStartOf="@+id/guideline14"
            android:layout_marginStart="8dp"
            android:textSize="14sp" android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/setFinalAff"
            app:layout_constraintEnd_toStartOf="@+id/setFinalAff" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="@string/verify"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/finalInfo" android:textSize="30sp" app:layout_constraintStart_toStartOf="@+id/guideline14"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:gravity="center"
            app:layout_constraintHorizontal_bias="0.497" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/setFinalName"
            app:layout_constraintEnd_toStartOf="@+id/guideline15"/>
    <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:id="@+id/setFinalName" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:background="@drawable/rounded_button"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/setFinalAff"
            app:layout_constraintEnd_toStartOf="@+id/guideline15" android:textColor="@color/black"/>
    <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:id="@+id/setFinalAff"
            android:background="@drawable/rounded_button"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline13" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline15"
            android:textColor="@color/black"/>
    <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:id="@+id/setFinalEnlist" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:background="@drawable/rounded_button"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline13" app:layout_constraintEnd_toStartOf="@+id/guideline15"
            android:textColor="@color/black"/>
    <Button
            android:text="@string/complete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonComplete"
            android:background="@drawable/rounded_button"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline"
            app:layout_constraintStart_toStartOf="@+id/guideline12"
            android:layout_marginStart="8dp" android:textColor="@color/black" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline15"/>
    <Button
            android:text="@string/init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonInit"
            android:background="@drawable/rounded_button"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline12"
            android:textColor="@color/black" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline14"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline14"
            app:layout_constraintGuide_percent="0.05"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline12"
            app:layout_constraintGuide_percent=".5"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline15" app:layout_constraintGuide_percent=".95"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline13"
            app:layout_constraintGuide_percent="0.500684"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.85"/>
</androidx.constraintlayout.widget.ConstraintLayout>