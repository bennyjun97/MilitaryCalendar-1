<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kyminbb.militarycalendar">
    <application
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".TabActivity"
                android:label="@string/title_activity_tab"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".activities.GraphActivity">
        </activity>
        <activity android:name=".activities.CalendarActivity">
        </activity>
        <activity android:name=".activities.SetEnlistActivity">
        </activity>
        <activity android:name=".activities.SetAffActivity">
        </activity>
        <activity android:name=".activities.SetNameActivity">
        </activity>
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".activities.SettingActivity"
                android:theme="@style/AppTheme.Setting"/>
    </application>
</manifest>